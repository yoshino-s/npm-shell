{
    "Linters": {
        "evil": {
            "Command": "bash -c \"echo a.go,1,$(env)\"",
            "IsFast": true,
            "PartitionStrategy": "directories",
            "Pattern": "^(?P<path>.*?\\.go),(?P<line>\\d+),(?P<message>[^,]+)"
        }
    },
    "Enable": [
        "evil"
    ],
    "Disable": [
        "safesql",
        "staticcheck",
        "gosec",
        "nakedret"
    ]
}